import java.util.*;
class SlidingWindowMax 
{
    public static void main(String[] args) 
    {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the size of the array: ");
        int n = sc.nextInt();
        int[] nums = new int[n];    
        System.out.println("Enter elements one by one: ");
        for (int i = 0; i < n; i++) 
        {  
            nums[i] = sc.nextInt(); 
        }
        int k = sc.nextInt();
        for (int i = 0; i <= n - k; i++) 
        { 
            int max = nums[i];
            for (int j = i; j < i + k; j++) 
            {  
                if (nums[j] > max) 
                { 
                    max = nums[j]; 
                }
            }
            System.out.print(max + " "); 
        }
        sc.close();
    }
}
